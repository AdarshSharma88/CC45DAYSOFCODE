#include <bits/stdc++.h> 
using namespace std; 


 // } Driver Code Ends

class Solution{
    public:
    string removeConsecutiveCharacter(string s)
    {
        char p[s.size()];
        strcpy(p,s.c_str());
    
        
     for(int i=0;i<=s.size();i++)
        {
            if(p[i] != p[i+1])
            {
                cout<<p[i];
                
             
            }
        }
}

};

// { Driver Code Starts.
int main() 
{ 
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        Solution ob;
        cout<<ob.removeConsecutiveCharacter(s)<<endl;
    }
} 
