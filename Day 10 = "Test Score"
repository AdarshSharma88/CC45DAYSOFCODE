#include <iostream>
using namespace std;

int main() {
    int T,N,X,Y;
    cin>>T;
    while(T--)
    {
        int a = 0;
        cin>>N>>X>>Y;
        for(int i=0;i<=N;i++)
        {
            if(X*i==Y)
            {
                a = 1;
            }
        }
        if(a==1)
        {
            cout<<"Yes"<<endl;
        }
        else
        {
            cout<<"No"<<endl;
        }
    }
	// your code goes here
	return 0;
}
